    <!-- Installation -->
    <string name="apply_update_dialog_title">আপডেট প্রয়োগ করা হচ্ছে</string>
    <string name="apply_update_dialog_message">আপনি আধুনিকরণ করতে চলেছেন <xliff:g id="update_name">%1$s</xliff:g>।\n\n আপনি যদি <xliff:g id="ok">%2$s</xliff:g> চাপেন, আধুনিকরণ স্থাপন করতে ডিভাইসটি নিজেই হবে পুনরুদ্ধারের অবস্থায় চালু।\n\nদ্রষ্টব্যঃ এই বৈশিষ্ট্যটির একটি উপযুক্ত পুনরুদ্ধার অবস্থার প্রয়োজন অথবা আপনাকে অস্বয়ংক্রিয় ভাবে স্থাপন করতে হবে।</string>
    <string name="apply_update_dialog_message_ab">আপনি আপডেট করতে চলেছেন <xliff:g id="update_name">%1$s</xliff:g>।\n\n আপনি যদি <xliff:g id="ok">%2$s</xliff:g> চাপেন, ডিভাইসটি ব্যাকগ্রাওউন্ডে আপডেটটি ইনস্টল করা শুরু করবে।\n\nসম্পূর্ণ হয়ে গেলে, আপনাকে রিবুট করার অনুরোধ জানানো হবে।</string>
